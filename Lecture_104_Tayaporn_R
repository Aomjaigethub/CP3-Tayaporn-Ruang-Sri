class Customer:
    def __init__(self, name, lastname, age):
        self.name = name
        self.lastname = lastname
        self.age = age

    def add_cart(self):
        print(self.name, self.lastname, "your book is added")


mom = Customer("aomjai", "hinsri", 53)
dad = Customer("aomjai", "hinsri", 48)
bigSis = Customer("aomjai", "hinsri", 28)
bigBro = Customer("Jay", "hinsri", 32)


mom.add_cart()
dad.add_cart()
bigBro.add_cart()
bigSis.add_cart()




